    <script>
        $(document).ready(function () {
            $(window).scroll(function () {
                if ($(this).scrollTop() > 20) {
                    $('#back-to-top').fadeIn();
                } else {
                    $('#back-to-top').fadeOut();
                }
            });

            $('#back-to-top').click(function () {
                $("html, body").animate({
                    scrollTop: 0
                }, 1000);
                return false;
            });
        });

        $(document).on('click', '.nav-item.has-treeview > a', function (e) {
            var $this = $(this);
            var href = $this.attr('href');
            if (href && href.trim() !== "") {
                window.location.href = href;
            }
        });
    </script>
