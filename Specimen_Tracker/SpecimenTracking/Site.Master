                    format: 'DD-MMM-YYYY',
                    yearRange: [1910, 2050],
                    maxDate: new Date()
                });
            });

            $('.txtTime').each(function (index, element) {
                $(element).inputmask(
                    "hh:mm", {
                    placeholder: "HH:MM",
                    insertMode: false,
                    showMaskOnHover: false,
                    hourFormat: "24"
                });
            });

            $('.initial').each(function (index, element) {
                $(element).inputmask({
                    mask: '***',
                    placeholder: '___',
                    casing: 'upper'
                });
            });

            $('.select2').select2({
                theme: 'bootstrap4'
            });

        }

        $(document).ready(function () {
