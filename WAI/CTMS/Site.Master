        // Flag to track if user selected "Cancel" to avoid repeated prompts
        let sessionExpirationScheduled = false;

        function checkSession() {
           
            // Get the session timeout value from the server-side (in minutes)
            var sessionTimeout = "<%= Session.Timeout %>";

            if (isNaN(sessionTimeout)) sessionTimeout = 20; // Default to 20 if not provided

            // Convert the session timeout to milliseconds
            var sessionTimeoutInMilliseconds = sessionTimeout * 60 * 1000;

            // Get the current time
            var currentTime = new Date().getTime();

            // Check if the sessionTimeout cookie exists
            var sessionCookie = document.cookie.match('(^|;) ?sessionTimeout=([^;]*)(;|$)');

            if (sessionCookie) {
                var sessionStartTime = new Date(sessionCookie[2]).getTime();
                var remainingTime = sessionTimeoutInMilliseconds - (currentTime - sessionStartTime);


                // Update the UI to show the remaining time
                $("#secondsIdle").html(msToTime(remainingTime));

                if (remainingTime > 0) {
                    if (!sessionExpirationScheduled && remainingTime <= 120000) {
                        // If remaining time is less than or equal to 2 minutes, show a warning
                        if (confirm("Your session is about to expire. Do you want to continue your session?")) {
                            // Reset the session timeout cookie
                            document.cookie = "sessionTimeout=" + new Date() + "; path=/";

                            // Reset the UI to show the full session timeout
                            $("#secondsIdle").html(msToTime(sessionTimeoutInMilliseconds));
                        } else {
                            // User chose "Cancel"; schedule session expiration and stop further prompts
                            sessionExpirationScheduled = true;
                            setTimeout(function () {
                                deleteCookie();
                                window.location = "SessionExpired.aspx";
                            }, remainingTime);
                        }
                    }
                } else {
                    // If session has expired, redirect to the session expired page
                    deleteCookie();
                    window.location = "SessionExpired.aspx";
                }
            } else {
                // Initialize the session timeout cookie if it doesn't exist
                document.cookie = "sessionTimeout=" + new Date() + "; path=/";
            }

            // Recheck the session every second
            setTimeout(checkSession, 1000); // Update every second to reflect the countdown
        }


        function msToTime(duration) {

            var seconds = parseInt((duration / 1000) % 60)
                , minutes = parseInt((duration / (1000 * 60)) % 60);

            minutes = (minutes < 10) ? "0" + minutes : minutes;
            seconds = (seconds < 10) ? "0" + seconds : seconds;

            return minutes + ":" + seconds;
        }

        // Call this function during logout
        function handleLogout() {
            deleteCookie(); // Clear the sessionTimeout cookie
        }

       
  

    </script>
