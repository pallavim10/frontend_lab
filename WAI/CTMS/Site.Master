        <script type="text/javascript">
          
            // Function to initialize the MaxLength functionality
            function initializeMaxLength() {
                $("textarea[maxlength], input[maxlength]").each(function () {
                    const input = this;
                    const maxLength = parseInt($(input).attr("maxlength"), 10);

                    // Create or update a label to show the remaining characters
                    if (!$(input).next('.char-count').length) {
                        $(input).after('<label class="char-count" style="margin-left: 10px; font-size: 12px; color: gray;"></label>');
                    }

                    // Calculate and display the remaining characters
                    updateRemainingCharacters(input, maxLength);

                    // Handle input event
                    $(input).on('input', function () {
                        updateRemainingCharacters(input, maxLength);

                        if (input.value.length > maxLength) {
                            alert(`Maximum limit of ${maxLength} characters exceeded!`);
                            input.value = input.value.substring(0, maxLength); // Trim excess
                        }
                    });

                    // Handle keypress to prevent further input when maxlength is reached
                    $(input).on('keypress', function (e) {
                        if (input.value.length >= maxLength && !e.ctrlKey && !e.metaKey && maxLength > 0) {
                            e.preventDefault(); // Prevent additional input
                            alert(`Maximum limit of ${maxLength} characters exceeded!`);
                        }
                    });
                });
            }

            // Function to update the remaining characters
            function updateRemainingCharacters(input, maxLength) {
                const remaining = maxLength - input.value.length;
                const displayRemaining = Math.max(0, remaining);
                $(input).next('.char-count').text(displayRemaining + ' characters left');
            }

            // Reapply functionality after UpdatePanel partial postbacks
            $(document).ready(function () {
                initializeMaxLength();

                if (typeof Sys !== 'undefined' && typeof Sys.WebForms !== 'undefined' && Sys.WebForms.PageRequestManager) {
                    Sys.WebForms.PageRequestManager.getInstance().add_endRequest(function () {
                        initializeMaxLength();
                    });
                }
            });
        </script>
